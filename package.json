{
  "name": "asciidoctor-chart",
  "scripts": {
    "bundle:examples:asciidoctor": "bundle exec asciidoctor -r asciidoctor-chart examples/*.adoc",
    "bundle:examples:install": "cd examples && bundle install",
    "cy:run": "npx cypress run",
    "cy:open": "npx cypress open",
    "cy:visual-regression-update-base": "npx cypress run --env type=base --config screenshotsFolder=cypress/snapshots/base",
    "precy:visual-regression": "npm run bundle:examples:install && npm run bundle:examples:asciidoctor",
    "cy:visual-regression": "npx cypress run --env type=actual",
    "test": "npm run cy:visual-regression"
  },
  "devDependencies": {
    "cypress": "10.0.3",
    "cypress-mochawesome-reporter": "^3.1.0",
    "cypress-multi-reporters": "^1.6.0",
    "cypress-visual-regression": "1.7.0",
    "mocha-junit-reporter": "^2.0.2"
  }
}
